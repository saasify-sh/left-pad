{
  "name": "saasify-left-pad",
  "version": "0.1.0",
  "private": true,
  "repository": "saasify-sh/left-pad",
  "author": "Saasify <dev@saasify.sh>",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node build/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "predev": "run-s build:routes",
    "clean": "del build dist",
    "build": "run-s build:*",
    "prebuild": "run-s clean",
    "build:swagger": "tsoa swagger",
    "build:routes": "tsoa routes",
    "build:ts": "tsc",
    "test": "run-s build",
    "deploy": "now",
    "predeploy": "run-s build",
    "postdeploy": "now rm --safe left-pad"
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-compress": "^3.0.0",
    "koa-router": "^8.0.8",
    "tsoa": "^2.5.13"
  },
  "devDependencies": {
    "@types/koa": "^2.11.2",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-compress": "^2.0.9",
    "@types/koa-router": "^7.4.0",
    "@types/koa__cors": "^3.0.1",
    "@types/node": "^12.12.31",
    "cross-env": "^7.0.2",
    "del-cli": "^3.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
